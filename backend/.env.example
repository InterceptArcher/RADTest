# FastAPI Backend Environment Variables
# IMPORTANT: This file is an example. Never commit actual secrets.
# Copy this file to .env and fill in the values from your secure environment.

# Application Settings
DEBUG=false
MAX_REQUEST_SIZE=1048576

# Railway Worker Service
# These values must be provided via environment variables
RAILWAY_WORKER_URL=
RAILWAY_API_TOKEN=

# Supabase Configuration
# These values must be provided via environment variables
SUPABASE_URL=
SUPABASE_KEY=

# Intelligence APIs
# These values must be provided via environment variables
APOLLO_API_KEY=
PDL_API_KEY=
GNEWS_API_KEY=

# ZoomInfo API (optional - system works without it)
# Preferred: set client_id + client_secret for automatic token refresh
ZOOMINFO_CLIENT_ID=
ZOOMINFO_CLIENT_SECRET=
# Alternative: set a static access token (expires after ~1 hour)
ZOOMINFO_ACCESS_TOKEN=

# LLM Provider
# This value must be provided via environment variables
OPENAI_API_KEY=

# Gamma API
# This value must be provided via environment variables
GAMMA_API_KEY=

# Timeout Settings
WORKER_TIMEOUT=300
